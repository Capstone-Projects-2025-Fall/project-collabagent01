{
  "name": "collab-agent01",
  "publisher": "capstone-team-02",
  "displayName": "Collab Agent 01",
  "description": "AI-powered collaboration assistant that integrates with Live Share to analyze team activity, provide real-time notifications about what teammates are working on, and enable intelligent team communication through a side panel interface",
  "version": "0.4.1",
  "license": "MIT",
  "keywords": [
    "collaboration",
    "team",
    "supabase",
    "github",
    "live share",
    "auth"
  ],
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onUri"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Capstone-Projects-2025-Fall/project-collabagent01"
  },
  "main": "./dist/extension.js",
  "contributes": {
    "keybindings": [],
    "configuration": [
      {
        "title": "Collab Agent",
        "properties": {
          "collabAgent.supabase.url": {
            "type": "string",
            "default": "https://mpldhvkywihmfjdwzakd.supabase.co",
            "description": "Supabase project URL (https://xxxxx.supabase.co). Required for authentication."
          },
          "collabAgent.supabase.anonKey": {
            "type": "string",
            "default": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZSIsInJlZiI6Im1wbGRodmt5d2lobWZqZHd6YWtkIiwicm9sZSI6ImFub24iLCJpYXQiOjE3NTk0NDQzOTQsImV4cCI6MjA3NTAyMDM5NH0.qaxjv1mjNdRrVbpG6dspzw37vNxxug_LHSBUZpwoBrM",
            "description": "Supabase anon/public API key required for authentication."
          },
          "collabAgent.general.vendor": {
            "type": "string",
            "default": "google",
            "enum": [
              "ollama",
              "openai",
              "google"
            ],
            "description": "Choose the vendor providing the model."
          },
          "collabAgent.general.modelSelection": {
            "type": "string",
            "default": "gemini-2.0-flash-lite",
            "enum": [
              "--Ollama--(Not currently supported)",
              "codellama:7b",
              "codellama:latest",
              "--Openai--",
              "gpt-4o-mini",
              "--Google--",
              "gemini-2.0-flash-lite"
            ],
            "description": "Select which LLM is used for suggestions.",
            "group": "general"
          },
          "collabAgent.debug.bugFlag": {
            "type": "boolean",
            "default": false,
            "description": "Check for every suggestion generated to contain a bug.",
            "group": "debug"
          }
        }
      }
    ],
    "commands": [
      {
        "command": "collabAgent.testFetch",
        "title": "Debug: Test fetchSuggestions"
      },
      {
        "command": "collabAgent.setDisplayName",
        "title": "Collab Agent: Set Display Name"
      },
      {
        "command": "collabAgent.signIn",
        "title": "Collab Agent: Sign In to Collab Agent"
      },
      {
        "command": "collabAgent.signOut",
        "title": "Collab Agent: Sign Out of Collab Agent"
      },
      {
        "command": "collabAgent.fetchSettings",
        "title": "Collab Agent: Fetch Settings"
      },
      {
        "command": "collabAgent.refreshPanel",
        "title": "Collab Agent: Refresh Panel"
      },
      {
        "command": "collabAgent.supabaseSignIn",
        "title": "Collab Agent: Sign In (OAuth)"
      },
      {
        "command": "collabAgent.authCallback",
        "title": "Handle Auth Callback"
      },
      {
        "command": "collabAgent.validateCurrentProject",
        "title": "Collab Agent: Validate Current Project"
      },
      {
        "command": "collabAgent.showCurrentProjectInfo",
        "title": "Collab Agent: Show Project Information"
      },
      {
        "command": "collabAgent.updateTeamProject",
        "title": "Collab Agent: Update Team Project"
      },
      {
        "command": "collabAgent.openTeamProject",
        "title": "Collab Agent: Open Team Project"
      }
    ],
    "views": {
      "collabAgent": [
        {
          "id": "collabAgent.teamActivity",
          "name": "Collab Agent",
          "icon": "$(organization)",
          "type": "webview"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "collabAgent",
          "title": "Collab Agent",
          "icon": "$(organization)"
        }
      ]
    }
  },
  "scripts": {
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "jest",
    "test:coverage": "jest --coverage",
  "test:coverage:docs": "npm run test:coverage && node copy-coverage.js && node generate-coverage-summary.js",
    "compile": "npm run check-types && node esbuild.js",
    "check-types": "tsc --noEmit",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "vscode:prepublish": "npm run package",
    "package": "npm run check-types && node esbuild.js --production",
  "vsce:login": "vsce login capstone-team-02",
    "vsce:package": "vsce package",
    "vsce:publish": "vsce publish",
    "docs:generate": "typedoc",
    "docs:serve": "npx http-server docs -p 8080 -o",
    "docs:clean": "rimraf docs"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "22.x",
    "@types/vscode": "^1.97.0",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "esbuild": "^0.25.1",
    "eslint": "^9.19.0",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "npx": "^10.2.2",
    "package": "^1.0.1",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typedoc": "^0.27.9",
    "typescript": "^5.7.3",
    "vsce": "2.15.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.58.0",
    "axios": "^1.7.9",
    "dotenv": "^17.2.3",
    "typedoc-theme-hierarchy": "^5.0.4",
    "vsls": "^1.0.4753"
  }
}
